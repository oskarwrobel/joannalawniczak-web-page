{
  "name": "joannalawniczak",
  "version": "0.0.1",
  "description": "Joanna Lawniczak home page.",
  "scripts": {
    "lint": "eslint --quiet '**/*.js'",
    "precommit": "lint-staged",
    "build": "webpack --config dev/webpack.config.js --mode development",
    "build:production": "webpack --config dev/webpack.config.js --mode production --optimize-minimize --env.analytics=UA-11153361-29",
    "serve": "http-server ./dist/",
    "deploy": "npm run lint && node ./dev/deploy"
  },
  "author": "Oskar Wrobel",
  "dependencies": {},
  "devDependencies": {
    "@oskarwrobel/deploy-tools": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^4.14.0",
    "eslint-config-ckeditor5": "^1.0.7",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "imports-loader": "^0.7.1",
    "lint-staged": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "dist/**",
    "src/scripts/analytics.js"
  ]
}
